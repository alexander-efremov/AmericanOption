    {
    X1 = 13
    Y1 = 10.892
    X2 = 88
    Y2 = 87.892
    }
  ViewportTopSnapTarget = 87.8918918919
$!XYLineAxis 
  XDetail 1
    {
    RangeMin = 0
    RangeMax = 1.000099999999999989
